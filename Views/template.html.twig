<!DOCTYPE html>
<html lang="fr">

<head>
   <meta charset="UTF-8">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <meta http-equiv="X-UA-Compatible" content="ie=edge">
   <link rel='icon' href='{{baseUrl}}/assets/avatar/jatilogo.png' type='image/x-icon'/ >
   <title>{% block title %}Allo Jati{% endblock %}</title>

   {% block stylesheets %}
   <link rel="stylesheet" href="{{baseUrl}}/assets/css/style.css">
   <link rel="stylesheet" href="{{baseUrl}}/assets/css/bootstrap+reboot.css">
   {% endblock %}
</head>

<body>
   {% block header %}

   <!-- HEADER VISITEUR  -->
   {% if status is null %}
   <header>
      <nav class="header-nav navbar navbar-expand-lg mb-0 p-0">
         <a class="navbar-brand ml-5" href="{{baseUrl}}">
            <img src="https://i.ibb.co/RSYk2Kw/jatilogo.png" width="100" height="100" class="d-inline-block align-top"
               alt="">
         </a>
         <div class="collapse navbar-collapse " id="navbarText">
            <ul class="navbar-nav mx-auto font-weight-bold p-0">
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Films">FILMS</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Artistes">ARTISTES</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Connection">CONNECTION</a>

               </li>
            </ul>
         </div>
      </nav>
   </header>

   <!-- HEADER ADMIN  -->
   {% elseif status == 1 %}
   <header>
      <nav class="header-nav navbar navbar-expand-lg mb-0 p-0">
         <a class="navbar-brand ml-5" href="{{baseUrl}}">
            <img src="https://i.ibb.co/RSYk2Kw/jatilogo.png" width="100" height="100" class="d-inline-block align-top"
               alt="">
         </a>
         <div class="collapse navbar-collapse " id="navbarText">
            <ul class="navbar-nav mx-auto font-weight-bold p-0">
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Films">FILMS</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Artistes">ARTISTES</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Deconnection">DECONNECTION</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Profile">PROFILE</a>
                  
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Admin">ADMIN</a>
               </li>
               <li>
                  {% if alertMessage != 0 %}
                  <div id="messages">

                  <img src="{{baseUrl}}/assets/avatar/notif.png" class="message">
                  <span class="alertMessage">{{ alertMessage}}</span>

                  </div>
                  {% endif %}
               </li>
            </ul>
         </div>
      </nav>
   </header>

   <!-- HEADER USER  -->
   {% elseif status == 2 %}
   <header>
      <nav class="header-nav navbar navbar-expand-lg mb-0 p-0">
         <a class="navbar-brand ml-5" href="{{baseUrl}}">
            <img src="https://i.ibb.co/RSYk2Kw/jatilogo.png" width="100" height="100" class="d-inline-block align-top"
               alt="">
         </a>
         <div class="collapse navbar-collapse " id="navbarText">
            <ul class="navbar-nav mx-auto font-weight-bold p-0">
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Films">FILMS</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Artistes">ARTISTES</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Suggestion">Suggestion</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Deconnection">DECONNECTION</a>
               </li>
               <li class="nav-item active">
                  <a class="nav-link hover-menu" href="{{baseUrl}}/Profile">PROFILE</a>
               </li>
               <li>
                  {% if alertMessage != 0 %}
                  <div id="messages">

                  <img src="{{baseUrl}}/assets/avatar/notif.png" class="message">
                  <span class="alertMessage">{{ alertMessage}}</span>

                  </div>
                  {% endif %}
               </li>
            </ul>
         </div>
      </nav>
   </header>
   {% endif %}
   {% endblock %}

   {% block body %}
      {# Here the main content, you can create other blocks if there are useful to you #}
   {% endblock %}

   {% block footer %}
      {# Footer ici #}
   {% endblock %}

   {% block javascripts %}
   <script src="{{baseUrl}}/assets/js/app.js"></script>
   <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
   <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
   <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
   {% endblock %}
</body>
</html>
